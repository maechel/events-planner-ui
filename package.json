        "@vue/tsconfig": "^0.8.1",
        "eslint": "^9.39.2",
        "eslint-plugin-vue": "^10.6.2",
        "globals": "^17.0.0",
        "husky": "^9.1.7",
        "jsdom": "^27.4.0",
        "lint-staged": "^16.2.7",
        "prettier": "3.7.4",
