        "@primeuix/themes": "^2.0.2",
        "@tailwindcss/forms": "^0.5.11",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.18",
        "@vue/test-utils": "^2.4.6",
        "@vueuse/core": "^14.1.0",
        "axios": "^1.13.2",
        "chart.js": "^4.5.1",
        "date-fns": "^4.1.0",
        "pinia": "^3.0.4",
        "primevue": "^4.5.4",
        "tailwindcss": "^4.1.18",
